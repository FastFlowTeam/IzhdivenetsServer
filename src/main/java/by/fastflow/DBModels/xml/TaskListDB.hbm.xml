<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.fastflow.DBModels.xml.TaskListDB" table="task_list" schema="izh_scheme" catalog="db">
        <id name="listId">
            <column name="list_id" sql-type="bigint"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="bigint" not-null="true"/>
        </property>
        <property name="visibility">
            <column name="visibility" sql-type="bigint"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="canWork">
            <column name="can_work" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>