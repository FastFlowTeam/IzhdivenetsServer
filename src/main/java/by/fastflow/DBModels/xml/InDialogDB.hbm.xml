<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.fastflow.DBModels.InDialogDB" table="in_dialog" schema="izh_scheme" catalog="db">
        <composite-id mapped="true" class="by.fastflow.DBModels.InDialogDBPK">
            <key-property name="dialogId">
                <column name="dialog_id" sql-type="bigint"/>
            </key-property>
            <key-property name="userId">
                <column name="user_id" sql-type="bigint"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>