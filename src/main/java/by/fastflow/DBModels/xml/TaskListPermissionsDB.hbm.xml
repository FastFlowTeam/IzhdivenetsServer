<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.fastflow.DBModels.xml.TaskListPermissionsDB" table="task_list_permissions" schema="izh_scheme"
           catalog="db">
        <composite-id mapped="true" class="by.fastflow.DBModels.xml.TaskListPermissionsDBPK">
            <key-property name="userId">
                <column name="user_id" sql-type="bigint"/>
            </key-property>
            <key-property name="listId">
                <column name="list_id" sql-type="bigint"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>