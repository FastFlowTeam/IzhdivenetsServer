<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="by.fastflow.DBModels.main.PushDB" table="push" schema="izh_scheme" catalog="db">
        <id name="token">
            <column name="token" sql-type="varchar(200)" length="200"/>
        </id>
        <property name="device">
            <column name="device" sql-type="bigint"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>